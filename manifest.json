{
  "manifest_version": 3,
  "name": "Sahibinden Boya Durumu",
  "version": "1.0",
  "description": "Araç listelerinde gezinirken boyalı ve değişen parçaları gösterir.",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "host_permissions": [
    "https://www.sahibinden.com/"
  ],
  "content_scripts": [
    {
      "matches": ["https://www.sahibinden.com/*"],
      "js": ["content.js"]
    }
  ]

}
